class vec2 {
  int x;
  int y;
  
  fn add(int x, int y) : vec2 {
    vec2 w;
    w.x = x + this->x;
    w.y = y + this->y;
    return w;
  }
  
  fn mul(int z) : vec2 {
    vec2 w;
    w.x = this->x * z;
    w.y = this->y * z;
    return w;
  }
  
  fn echo() {
    print this->x;
    print this->y;
  }
}

fn vec2_new(int x, int y) : vec2 {
  vec2 v;
  v.x = x;
  v.y = y;
  return v;
}

vec2_new(3, 4).add(3, 4).mul(3).echo();
