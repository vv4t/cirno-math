
class vec2 {
  int x;
  int y;
}

fn add(vec2 a, vec2 b) : vec2 {
  vec2 c;
  c.x = a.x + b.x;
  c.y = a.y + b.y;
  return c;
}

fn sub(vec2 a, vec2 b) : vec2 {
  vec2 c;
  c.x = a.x - b.x;
  c.y = a.y - b.y;
  return c;
}

vec2 z;
z.x = 111;
z.y = 222;

vec2 w;
w.x = 333;
w.y = 444;

vec2 u = sub(z, w);
print u.x;
print u.y;
